}
<div class="col-main col-xs-12 col-sm-9 cusmin-height">

    @Html.Partial("~/Views/Default/Defaultpaging.cshtml", dataNew.ToPagedList(1, 10))
    @Html.Partial("~/Views/NewOrther/NewOrtherPaging.cshtml", dataNewOrther.ToPagedList(1, 10))

